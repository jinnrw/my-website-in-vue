    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "firebase": "^5.11.1",
    "vue": "^2.6.11",
    "vue-gtag": "^1.1.2",
    "vue-router": "^3.1.3",
    "vuefire": "^1.4.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.1.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
